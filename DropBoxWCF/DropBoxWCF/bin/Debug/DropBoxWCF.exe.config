<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IAccount"/>
                <binding name="NetTcpBinding_ICloud"/>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost/DropBox/Account" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IAccount" contract="ServiceReference1.IAccount" name="NetTcpBinding_IAccount">
            </endpoint>
            <endpoint address="net.tcp://localhost/DropBox/Cloud" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICloud" contract="ServiceReference1.ICloud" name="NetTcpBinding_ICloud">
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>
